<%= form_with model: @currency do |form| %>

  <div>
    <%= form.label :code %><br>
    <%= form.text_field :code %>
  </div>

  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %><br>
  </div>

  <div>
    <%= form.label :symbol %><br>
    <%= form.text_field :symbol %><br>
  </div>

  <br>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<br>
<%= link_to "Back to owner", owners_path() %>
